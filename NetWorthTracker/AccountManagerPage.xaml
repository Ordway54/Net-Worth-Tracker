<Page x:Class="NetWorthTracker.AccountManagerPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:NetWorthTracker"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="AccountManagerPage">

    <Grid Background="White">
        <Grid.RowDefinitions>
            <RowDefinition Height="50" />
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Label Content="Manage Accounts" FontSize="24"></Label>
        <StackPanel Grid.Row="1">
            <Label Content="Account 1"></Label>
            <Label Content="Account 2"></Label>
            <Label Content="Account 3"></Label>
            <Label Content="Account 4"></Label>
            <Label Content="Account 5"></Label>
        </StackPanel>
        
    </Grid>
</Page>
